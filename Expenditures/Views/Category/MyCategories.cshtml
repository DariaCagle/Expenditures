@using Expenditures.Models.Category

@model CategoryViewModel

@{
    ViewBag.Title = "Index";

}

<h2>I am on index category @Model.Title</h2>

@if (!string.IsNullOrEmpty(Model.Title))
{
    <h1>This title is not empty.</h1>
}
else
{
    <h1>This title is empty.</h1>
}
<div id="transactionDetails"></div>

@Ajax.ActionLink("Get Details", "GetTransactionList", new AjaxOptions { HttpMethod = "GET", UpdateTargetId = "transactionDetails", InsertionMode = InsertionMode.Replace })

<script src="https://code.jquery.com/jquery-3.5.1.min.js"
        integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0="
        crossorigin="anonymous"></script>


<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-ajax-unobtrusive/3.2.6/jquery.unobtrusive-ajax.min.js"></script>